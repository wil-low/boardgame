        <PAGEOBJECT XPOS="406.607142857143" YPOS="3846.37700787402" OwnPage="4" ItemID="835363524" PTYPE="4" WIDTH="246.428571428571" HEIGHT="285.051563554555" FRTYPE="3" CLIPEDIT="1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L246.18 0 L246.429 285.052 L0 285.052 L0 0 Z" copath="M0 0 L246.18 0 L246.18 285.052 L0 285.052 L0 0 Z" gXpos="0" gYpos="0" gWidth="0" gHeight="0" ALIGN="3" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle ALIGN="3" FONTSIZE="10"/>
                <ITEXT FONT="Times New Roman Bold" FONTSIZE="10" CH="Ресурсы:"/>
                <ITEXT FONTSIZE="10" CH=" ресурсы размещают на планшете игрока или на некоторых картах. В игре присутствуют различные ресурсы (см. стр. 14), все они обозначаются кубиками:"/>
                <para ALIGN="3"/>
                <ITEXT FONTSIZE="10" CH="БРОНЗОВЫЙ = 1"/>
                <para ALIGN="3"/>
                <ITEXT FONTSIZE="10" CH="СЕРЕБРЯНЫЙ = 5"/>
                <para ALIGN="3"/>
                <ITEXT FONTSIZE="10" CH="ЗОЛОТОЙ = 10"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Times New Roman Bold" FONTSIZE="10" CH="Маркеры игрока:"/>
                <ITEXT FONTSIZE="10" CH=" каждый игрок использует кубики своего цвета, чтобы отмечать ТР, владение тайлами, производство (см. стр 6) и использованные действия синих карт (см. стр. 11)."/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Times New Roman Bold" FONTSIZE="10" CH="Маркеры температуры, кислорода и поколения:"/>
                <ITEXT FONTSIZE="10" CH=" устанавливаются на соответствующих шкалах (см. стр. 4)."/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Times New Roman Bold" FONTSIZE="10" CH="Маркер первого игрока:"/>
                <ITEXT FONTSIZE="10" CH=" передается по часовой стрелке каждый ход."/>
                <trail ALIGN="3"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="179.370708661417" YPOS="4329.16787401575" OwnPage="5" ItemID="776740004" PTYPE="4" WIDTH="436.165005624297" HEIGHT="25.511811023622" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L436.165 0 L436.165 25.5118 L0 25.5118 L0 0 Z" copath="M0 0 L436.165 0 L436.165 25.5118 L0 25.5118 L0 0 Z" gXpos="0" gYpos="0" gWidth="0" gHeight="0" PSTYLE="Заголовок" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
