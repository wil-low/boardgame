            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Для вашей первой игры используйте эти карты"/>
                <para PARENT="Title2" ALIGN="1"/>
                <ITEXT FONT="Myriad Pro Semibold" CH="Цель и колода эпизода A:"/>
                <ITEXT CH=" &quot;Серенити (Часть 1)&quot;"/>
                <para PARENT="DefaultNoIndent"/>
                <ITEXT FONT="Myriad Pro Semibold" CH="Цель и колода эпизода B:"/>
                <ITEXT CH=" &quot;Серенити (Часть 2)&quot;"/>
                <para PARENT="DefaultNoIndent"/>
                <ITEXT FONT="Myriad Pro Semibold" CH="Цель и колода эпизода C:"/>
                <ITEXT CH=" &quot;Ограбление поезда&quot;"/>
                <para PARENT="DefaultNoIndent"/>
                <ITEXT FONT="Myriad Pro Semibold" CH="Игроки:"/>
