        <PAGEOBJECT XPOS="410.43" YPOS="1196.86779527062" OwnPage="1" ItemID="452281936" PTYPE="4" WIDTH="141.375" HEIGHT="14.0059055118118" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="1" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L141.375 0 L141.375 14.0059 L0 14.0059 L0 0 Z" copath="M0 0 L141.375 0 L141.375 14.0059 L0 14.0059 L0 0 Z" gXpos="332.240078740157" gYpos="275.134566924166" gWidth="495.6" gHeight="354.48" PSTYLE="DetStoryTitle" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
